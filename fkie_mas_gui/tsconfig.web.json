{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "vite-env.d.ts",
    "src/index.d.ts",
    "src/preload/**/*.ts",
    "src/preload/*.d.ts",
    "src/renderer/**/*",
    "src/renderer/**/*.tsx",
    "src/renderer/**/**/*.tsx",
    "src/renderer/**/**/**/*.tsx",
    "src/types/**/*.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "skipLibCheck": false
  }
}
