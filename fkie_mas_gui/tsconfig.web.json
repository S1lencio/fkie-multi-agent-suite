{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": ["src/renderer/**/*", "src/renderer/**/*.jsx", "src/renderer/**/*.tsx", "src/renderer/**/**/*.tsx", "src/preload/*.d.ts"],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": ["src/renderer/*"]
    },
    "allowJs": true,
    "lib": ["ES2021"]
  }
}
